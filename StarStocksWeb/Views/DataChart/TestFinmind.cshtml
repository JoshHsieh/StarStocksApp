@{
    ViewData["Title"] = "Test Finmind";
}

@model StarStocksWeb.Frameworks.ViewModels.FinmindViewModel

@section AddToHead
{
    <!--  修改 fileinput buuton text  -->
    <style type="text/css">
        .custom-file-input:lang(en) ~ .custom-file-label::after {
            content: "瀏覽" !important;
        }
    </style>
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Upload Files</h1>
        @*<a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i class="fas fa-download fa-sm text-white-50"></i> Generate Report</a>*@
    </div>

    <div class="row">
        <div class="col">
            <div class="card-body">
                @foreach ( var item in Model.PriceList )
                {
                    <span> @item.Time</span><span> @item.DealPrice</span>
                }
            </div>
        </div>
    </div>
</div>

@section scripts
{

}
